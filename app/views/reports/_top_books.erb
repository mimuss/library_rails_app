
<table class="table table-hover">
  <thead>
  <tr>
    <th>Название</th>
    <th>Шифр</th>
    <th>Автор</th>
    <th>Издательство</th>
    <th>Цена </th>
    <th>Кол-во выданных</th>
  </tr>
  </thead>
  <tbody>
  <% books.each do |key, value| %>
    <tr>
      <% key.each do |val| %>
        <td><%= val %></td>
      <% end %>
      <td><%= value %></td>
    </tr>
  <% end %>
  </tbody>
</table>